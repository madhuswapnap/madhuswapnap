#include<stdio.h>
int main()
{
  int j = 0,i,n,a[100];  
  printf("enter array size");
  scanf("%d",&n);
  printf("enter elements into array");
  for(i=0;i<n;i++)
  scanf("%d",&a[i]);
  for (i = 0; i < n; i++)
            if (a[i] != 0)
                a[j++] = a[i]; 
        while (j < n)
            a[j++] = 0;
            printf("output is:");
    for (int i = 0; i < n; i++)
      printf("%d ", a[i]);
  return 0;
}
